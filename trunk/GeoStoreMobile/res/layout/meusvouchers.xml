<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:padding="15dip"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_marginTop="5dp"
  android:stretchColumns="1">    
    <TableRow>
	    <TextView 
	    	android:id="@+id/tvNomeProdMeusVouchers"
	    	android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:textColor="@color/azul_2"
		    android:textSize="16dp">
	    </TextView>
	    <TextView 
	    	android:id="@+id/tvDescProdMeusVouchers"
	    	android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:textColor="@color/azul_2"
		    android:textSize="16dp">
	    </TextView>]
    </TableRow>		    
<TableRow>
	    <TextView 
	    	android:id="@+id/tvNomeLojaMeusVouchers"
	    	android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:textColor="@color/azul_2"
		    android:textSize="16dp">
	    </TextView>
	    <TextView 
	    	android:id="@+id/tvEndLojaMeusVouchers"
	    	android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:textColor="@color/azul_2"
		    android:textSize="16dp">
	    </TextView>
    </TableRow>		    
<TableRow>
	    <TextView 
	    	android:id="@+id/tvPrcProdMeusVouchers"
	    	android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:textColor="@color/azul_2"
		    android:textSize="16dp">
	    </TextView>
	    <TextView 
	    	android:id="@+id/tvNumVoucherMeusVouchers"
	    	android:layout_width="wrap_content" 
		    android:layout_height="wrap_content"
		    android:textColor="@color/azul_2"
		    android:textSize="16dp">
	    </TextView>						    		    	   
    </TableRow>	    
</TableLayout>